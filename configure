    conftest$ac_exeext conftest.$ac_ext
if test $have_set_uncaught_exception_handler = yes; then

printf "%s\n" "#define HAVE_SET_UNCAUGHT_EXCEPTION_HANDLER 1" >>confdefs.h

fi

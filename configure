/* end confdefs.h.  */
#include "$srcdir/config/config.setUncaughtExceptionHandler.m"
_ACEOF
if ac_fn_c_try_link "$LINENO"
then :
  have_set_uncaught_exception_handler=yes
else $as_nop
  have_set_uncaught_exception_handler=no
